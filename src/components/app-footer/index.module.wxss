.wrapper {
  position: relative;
  height: 172px;
  border-top: 1px solid #d3d3d3;
  overflow: hidden;
  background-color: #f2f2f2;
}
.wrapper .center {
  height: 115px;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.wrapper .center .left_box {
  width: 520px;
}
.wrapper .center .left_box .left_box_title {
  display: flex;
}
.wrapper .center .left_box .left_box_title li:nth-child(2n) {
  margin: 0 5px;
}
.wrapper .center .left_box .left_box_title li a {
  color: #959595;
}
.wrapper .center .left_box .left_box_title li span {
  color: #959595;
}
.wrapper .center .left_box .left_box_belong {
  margin: 5px 0;
}
.wrapper .center .left_box .left_box_belong span:nth-child(1) {
  margin-right: 20px;
}
.wrapper .center .left_box .left_box_safe {
  margin: 5px 0;
}
.wrapper .center .left_box .left_box_safe a:nth-child(1) {
  margin-right: 20px;
}
.wrapper .center .right_box {
  width: 420px;
}
.wrapper .center .right_box ul {
  display: flex;
  justify-content: space-around;
  align-items: center;
  text-align: center;
}
.wrapper .center .right_box ul li .image {
  background: url('@/assets/image/sprite_footer_02.png') no-repeat 0 0;
  background-size: 104px 444px;
  width: 44px;
  height: 44px;
  margin: 5px auto;
}
